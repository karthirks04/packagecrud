{
    "name": "laravelmoduelscrud/laravelpermissioneditor",
    "description": "A contact form packafge for laravel",
    "type": "metapackage",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "laravelmoduelscrud\\laravelpermissioneditor\\": "laravelmoduelscrud/laravelpermissioneditor/src/",
             "laravelmoduelscrud\\laravelpermissioneditor\\": "src/"
        }
    },
  
    "authors": [
        {
            "name": "karthicks"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
    "post-install-cmd": [
        "@php artisan migrate --path=packages/laravelmoduelscrud/laravelpermissioneditor/Database/migrations",
         "@php artisan vendor:publish --tag=your-package-tag --force",
         "@php artisan storage:link"
    ],
    "post-update-cmd": [
        "@php artisan migrate --path=packages/laravelmoduelscrud/laravelpermissioneditor/Database/migrations",
        "@php artisan vendor:publish --tag=your-package-tag --force",
        "@php artisan storage:link"
    ]
},
 "require": {
        "php": "^7.2.5|^8.0"
        
    }
  
}
